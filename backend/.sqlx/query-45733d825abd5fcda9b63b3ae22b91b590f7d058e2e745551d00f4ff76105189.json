{
  "db_name": "PostgreSQL",
  "query": "\n            WITH\n                dummy as (SELECT 1)\n                \n                \n                \n                \n                    , tag_filter as (\n                        SELECT\n                            recipe_id as id,\n                            count(*) as rank\n                        FROM\n                            recipes_tags\n                        WHERE tag_id = any($1)\n                        GROUP BY id\n                    )\n                \n                \n                \n                \n            SELECT\n                r.id,\n                r.name,\n                r.image,\n                is_recipe_in_account_favs(r.id, $2) as \"is_favorite!\",\n                r.ingredients as \"ingredients: Vec<QIngredient>\",\n                r.diets as \"diets: Vec<Diet>\",\n                n_shares,\n                is_private,\n                count(*) OVER() AS \"total_count!\"\n            FROM recipes_full AS r\n                \n                \n                \n                 INNER JOIN tag_filter as tf USING (id)\n                \n                \n                \n            WHERE\n                is_private = 'f' OR author_id = $2\n            ORDER BY\n                \n                \n                tf.rank DESC, \n                (extract(epoch from publication_date)+id)::bigint % $3\n            OFFSET $4\n            LIMIT $5\n        ",
